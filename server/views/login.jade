extends layout

block head
  meta(name="google-signin-clientid" content="998299604881-tsg6j85jdaen39pcj6hl06gqjg0l2m6r.apps.googleusercontent.com")
  meta(name="google-signin-scope" content="https://www.googleapis.com/auth/plus.login")
  meta(name="google-signin-requestvisibleactions" content="http://schema.org/AddAction")
  meta(name="google-signin-cookiepolicy" content="single_host_origin")
  meta(name="google-signin-callback" content="signinCallback")
  script(src="https://apis.google.com/js/client:platform.js?onload=render" async defer)
  script(src='/javascripts/g-login.js')

block content
  form(method="post" action="/")
                table
                  tr
                    th
                      label(for="email") Email:
                    td
                      input(type="text" id="email" name="email")
                  tr
                    th
                      label(for="password") Password:
                    td
                      input(type="password" id="password" name="password")
                input(type="submit" value="Submit")
              a(class="tiny" href="#s") Forgot password?
  #gSignInWrapper
    #myButton
